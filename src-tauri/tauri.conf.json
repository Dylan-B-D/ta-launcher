{
  "productName": "ta-launcherv2",
  "version": "0.0.0",
  "identifier": "TALauncherV2",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "T:A Launcher V2",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../public/configs/dodgehigh/*",
      "../public/configs/evil/*",
      "../public/configs/evillow/*",
      "../public/configs/veryhigh/*",
      "../public/configs/verylow/*",
      "../public/configs/defaultinput/*",
      "../public/configs/defaultini/*",
      "../public/configs/defaulttribeshelptext/*",
      "../public/configs/defaulttribesuser/*"
    ]
  }
}
